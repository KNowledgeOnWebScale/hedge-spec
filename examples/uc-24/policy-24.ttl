@prefix dcterms:       <http://purl.org/dc/terms/> .
@prefix dpv:           <https://w3id.org/dpv#> .
@prefix dpv-odrl:      <https://w3id.org/dpv/mappings/odrl#> .
@prefix ex:            <https://example.org/> .
@prefix odrl:          <http://www.w3.org/ns/odrl/2/> .
@prefix sector-health: <https://w3id.org/dpv/sector/health#> .

ex:uc24-p-sensorSlice-serviceProvision-patientDC-readModify a odrl:Set ;
    odrl:uid ex:uc24-p-sensorSlice-serviceProvision-patientDC-readModify ;
    dcterms:description "User allows his own MoveUp profile read and write (modify) access to his sensor slice."@en ;
    odrl:permission [
        odrl:action odrl:read, odrl:modify ; 
        odrl:target <https://faqir.org/patientPod/sensor/slice> ; 
        odrl:assigner ex:patient ;
        dpv:DataSubject ex:patient ;
        odrl:constraint [
            odrl:leftOperand dpv-odrl:Purpose ;
            odrl:operator odrl:isAnyOf ;
            odrl:rightOperand dpv:ServiceProvision, dpv:AccountManagement, 
            sector-health:ResourceManagement ] ] .