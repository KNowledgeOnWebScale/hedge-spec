@prefix dcterms:       <http://purl.org/dc/terms/> .
@prefix dpv:           <https://w3id.org/dpv#> .
@prefix dpv-odrl:      <https://w3id.org/dpv/mappings/odrl#> .
@prefix eu-gdpr:       <https://w3id.org/dpv/legal/eu/gdpr#> .
@prefix ex:            <https://example.org/> .
@prefix faqir:         <https://faqir.org/> .
@prefix odrl:          <http://www.w3.org/ns/odrl/2/> .
@prefix sotw:          <https://w3id.org/force/sotw#> .
@prefix xsd:           <http://www.w3.org/2001/XMLSchema#> .

ex:uc23-r-sensorSlice-serviceProvision-faqirDC-read-faqirAggregator a sotw:EvaluationRequest ;
    dcterms:issued "2024-02-12T11:20:10.999Z"^^xsd:dateTime ;
    dcterms:description "FAQIR Aggregator requests read access to the patient's sensor slice. With consent as legal basis." ;
    sotw:requestedAction odrl:read ;
    sotw:requestedTarget <https://faqir.org/patientPod/sensor/slice> ;
    sotw:requestingParty faqir:aggregator ;
    sotw:context [
        odrl:leftOperand dpv-odrl:Purpose ;
        odrl:operator odrl:eq ;
        odrl:rightOperand dpv:ServiceProvision ], [
        odrl:leftOperand dpv-odrl:LegalBasis ;
        odrl:operator odrl:eq ;
        odrl:rightOperand eu-gdpr:A9-2-a ] .

faqir:aggregator a dpv:ServiceProvider .
