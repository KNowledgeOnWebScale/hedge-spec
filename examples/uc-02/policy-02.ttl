@prefix dcterms:       <http://purl.org/dc/terms/> .
@prefix dpv:           <https://w3id.org/dpv#> .
@prefix dpv-odrl:      <https://w3id.org/dpv/mappings/odrl#> .
@prefix ex:            <https://example.org/> .
@prefix odrl:          <http://www.w3.org/ns/odrl/2/> .
@prefix sector-health: <https://w3id.org/dpv/sector/health#> .

ex:uc02-p-weight-primarycare-physicianDC-read a odrl:Set ;
    odrl:uid ex:uc02-p-weight-primarycare-physicianDC-read ;
    odrl:profile dpv-odrl: ;
    dcterms:description "Patient allows her weight subset of health data to be read for primary care. Known URI."@en ;
    odrl:permission [
        odrl:action odrl:read ;
        odrl:target ex:weight-by-hcp ; 
        odrl:assigner ex:patient ;
        odrl:constraint [
            odrl:leftOperand dpv-odrl:Purpose ;
            odrl:operator odrl:eq ;
            odrl:rightOperand sector-health:PrimaryCareManagement ] ] .
